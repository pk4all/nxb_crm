    <!-- ============================================================== -->
            <!-- Start right Content here -->
            <!-- ============================================================== -->
<div class="content">
    <div class="container-fluid">
        <!-- Page-Title -->
        <div class="row">
            <div class="col-sm-12">
                <h4 class="page-title">Data</h4>
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="#">Form</a></li>
                    <li class="breadcrumb-item"><a href="#">Settings</a></li>
                    <li class="breadcrumb-item active">Subscribers</li>
                </ol>
            </div>
        </div>
        {{#if flash.error}}
            <div class="row">
                <div class="alert alert-danger">
                {{{flash.error}}}
                </div>
            </div>
        {{/if}}
        {{#if flash.success}}
        <div class="alert alert-success" role="alert">
            {{{flash.success}}}
        </div>
        {{/if}}
        <div class="row">
            <div class="col-lg-12">
                <div class="card-box ">
                    <h4 class="text-dark header-title m-t-0">Email Data</h4>
                     <div class="table-responsive">
                                                <table class="table table-actions-bar">
                                                    <thead>
                                                    <tr>
                                                        <th>Sr. No.</th>
                                                        <th>Name</th>
                                                        <th>Email</th>
                                                        <th>Phone</th>
                                                        <th>Pin</th>
                                                        <th>Created</th>
                                                        {{!-- <th>Action</th> --}}
                                                    </tr>
                                                    </thead>
                                                    <tbody>
                                                    {{#if datas.length }}
                                                    {{#each datas }}
                                                     
                                                   <tr>
                                                        <td>{{incrementedIndex @index}}</td>
                                                        <td>{{#if this.name}}{{this.name}}{{else}}NA{{/if}}</td>
                                                       
                                                        <td>
                                                                {{#if this.email}}{{this.email}}{{else}}NA{{/if}}
                                                        </td>
                                                        <td>
                                                              {{#if this.phone}}{{this.phone}}{{else}}NA{{/if}}
                                                        </td>
                                                        <td>
                                                            {{#if this.pincode}}{{this.pincode}}{{else}}NA{{/if}}
                                                        </td>
                                                        <td>{{formatDate this?.createdAt}}</td>
                                                        {{!-- <td>
                                                            <a href="/admin/ads/edit/{{this._id}}" class="btn btn-success waves-effect waves-light"><i class="md md-edit"></i></a>

                                                            <form action="/admin/ads/delete/{{this._id}}" method="POST" style="display:inline;">
                                                                <input type="hidden" name="_method" value="DELETE">
                                                                <button class="btn btn-danger waves-effect waves-light" type="submit"><i class="md md-close"></i></button>
                                                            </form>
                                                        </td> --}}
                                                    </tr>
                                                   {{/each}}
                                                   {{else}}
                                                   <h4 class="text-dark header-title m-t-0">No Data found</h4>
                                                   {{/if}}
                                                    </tbody>
                                                </table>
                                        </div>
                                        {{> paging pagination=pagination}}          
                   
                </div>
                <!-- /Portlet -->
            </div>
        </div><!-- end row -->
    </div> <!-- container -->
</div> <!-- content -->

               


